        this.getactivitydetails();
    }
    getactivitydetails() {
        let dict = {
            id: this.activityid,
        };
        this.common.presentLoading();
        this.api.post('activitydetail', dict, '').subscribe((result) => {
            this.common.stopLoading();
            var res;
            res = result;
            if (res.status == 1) {
                this.activity = res.data;
            }
            else {
            }
        }, err => {
        });
    }

        this.show = !this.show;
        // CHANGE THE NAME OF THE BUTTON.
        if (this.show) {
            this.buttonName = "Hide";
            jquery__WEBPACK_IMPORTED_MODULE_8__('.plans_list' + id).addClass("active_plan");
